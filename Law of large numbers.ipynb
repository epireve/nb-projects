    "import numpy as np\n",
    "from numpy.random import randn\n",
    "\n",
    "N = 1000\n",
    "counter = 0\n",
    "\n",
    "for i in randn(N):\n",
    "    #print(\"Checking..\",i)\n",
    "    if (i > -1 and i < 1):\n",
    "        counter = counter + 1\n",
    "        \n",
    "counter / N\n",
    "        "
   ]
